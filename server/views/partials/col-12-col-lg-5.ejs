
<!-- Single Featured Post, skal loope 2 gange -->

<% if(data.featured2 != 'undefined') { %>
    <% data.featured2.forEach(item => { %>
<div class="single-blog-post featured-post-2">
    <div class="post-thumb">
        <a href="#"><img src="img/bg-img/17.jpg" alt=""></a>
    </div>
    <div class="post-data">
        <a href="#" class="post-catagory"> FEATURED2 <%= item.category %> </a>
        <div class="post-meta">
            <a href="#" class="post-title">
                <h6> <%= item.text %> </h6>
            </a>
            <!-- Post Like & Post Comment -->
            <div class="d-flex align-items-center">
                <a href="#" class="post-like"><img src="img/core-img/like.png" alt="">
                    <span>392</span></a>
                <a href="#" class="post-comment"><img src="img/core-img/chat.png" alt="">
                    <span>10</span></a>
            </div>
        </div>
    </div>
</div>

<% })  %>
<% } %> 
